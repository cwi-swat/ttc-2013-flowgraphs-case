<?xml version="1.0" encoding="ASCII"?>
<xmi:XMI xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:flowgraph="http://flowgraph/1.0">
  <flowgraph:Method txt="testMethod()" cfNext="/2">
    <stmts xsi:type="flowgraph:SimpleStmt" txt="int a = 1;"/>
    <stmts xsi:type="flowgraph:SimpleStmt" txt="int b = 2;"/>
    <stmts xsi:type="flowgraph:SimpleStmt" txt="int c = a + b;"/>
    <stmts xsi:type="flowgraph:SimpleStmt" txt="a = c;"/>
    <stmts xsi:type="flowgraph:SimpleStmt" txt="b = a;"/>
    <stmts xsi:type="flowgraph:SimpleStmt" txt="c = a / b;"/>
    <stmts xsi:type="flowgraph:SimpleStmt" txt="b = a - b;"/>
    <stmts xsi:type="flowgraph:Return" txt="return b * c;"/>
    <exit txt="Exit"/>
  </flowgraph:Method>
  <flowgraph:Exit txt="Exit" cfPrev="/9"/>
  <flowgraph:SimpleStmt txt="int a = 1;" cfNext="/3" cfPrev="/0" dfNext="/4"/>
  <flowgraph:SimpleStmt txt="int b = 2;" cfNext="/4" cfPrev="/2" dfNext="/4"/>
  <flowgraph:SimpleStmt txt="int c = a + b;" cfNext="/5" cfPrev="/3" dfNext="/5"/>
  <flowgraph:SimpleStmt txt="a = c;" cfNext="/6" cfPrev="/4" dfNext="/6 /7 /8"/>
  <flowgraph:SimpleStmt txt="b = a;" cfNext="/7" cfPrev="/5" dfNext="/7 /8"/>
  <flowgraph:SimpleStmt txt="c = a / b;" cfNext="/8" cfPrev="/6" dfNext="/9"/>
  <flowgraph:SimpleStmt txt="b = a - b;" cfNext="/9" cfPrev="/7" dfNext="/9"/>
  <flowgraph:Return txt="return b * c;" cfNext="/1" cfPrev="/8"/>
</xmi:XMI>
